{
  "name": "qase-playwright",
  "version": "2.0.16",
  "description": "A demo repository with Examples, for troubleshooting problems and for feature requests.",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:qase": "QASE_MODE=testops npx playwright test --config=playwright.qase.config.js",
    "test:parallel": "npx playwright test --workers=4",
    "test:shard": "npx playwright test --shard=1/4",
    "test:html": "npx playwright test --config=playwright.html.config.js --reporter=html",
    "test:html-shard": "npx playwright test --config=playwright.html.config.js --shard=1/4",
    "test:merge-reports": "npx playwright merge-reports --reporter html ./all-blob-reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cskmnrpt/qase-playwright.git"
  },
  "keywords": [
    "qase-playwright",
    "qaseio",
    "qase-tms",
    "qase-javascript"
  ],
  "author": "Qase Support",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/cskmnrpt/qase-playwright/issues"
  },
  "homepage": "https://github.com/cskmnrpt/qase-playwright#readme",
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "dotenv": "^16.4.7",
    "playwright-qase-reporter": "^2.1.6",
    "prettier": "^3.4.2"
  }
}
